<style>
  /* Allow vertical scrolling on mobile */
  @media (max-width: 767.98px) {
    /* Completely disable owl-carousel touch interactions */
    .hero,
    .home-slider,
    .home-slider * {
      touch-action: pan-y !important;
      -ms-touch-action: pan-y !important;
      pointer-events: auto !important;
    }
    
    /* Make owl-carousel non-interactive but allow scrolling through it */
    .home-slider .owl-stage-outer,
    .home-slider .owl-stage,
    .home-slider .owl-item {
      touch-action: pan-y !important;
      -ms-touch-action: pan-y !important;
      overflow: visible !important;
      -webkit-overflow-scrolling: touch;
    }
    
    /* Ensure hero doesn't block scrolling */
    .hero {
      position: relative !important;
      overflow: visible !important;
      touch-action: pan-y !important;
      -ms-touch-action: pan-y !important;
    }
    
    /* Ensure body can scroll */
    html {
      overflow-x: hidden !important;
      overflow-y: auto !important;
      -webkit-overflow-scrolling: touch;
      height: auto !important;
    }
    
    body {
      overflow-x: hidden !important;
      overflow-y: auto !important;
      -webkit-overflow-scrolling: touch;
      position: relative !important;
      height: auto !important;
    }
  }
</style>

<script is:inline>
  // Prevent owl-carousel from blocking vertical scrolling on mobile
  if (window.innerWidth <= 767) {
    document.addEventListener('DOMContentLoaded', function() {
      setTimeout(function() {
        var homeSlider = document.querySelector('.home-slider');
        if (homeSlider) {
          var owlStageOuter = homeSlider.querySelector('.owl-stage-outer');
          if (owlStageOuter) {
            var startY, startX;
            
            // Allow vertical scrolling through owl-carousel
            owlStageOuter.addEventListener('touchstart', function(e) {
              if (e.touches.length === 1) {
                startY = e.touches[0].clientY;
                startX = e.touches[0].clientX;
              }
            }, { passive: true });
            
            owlStageOuter.addEventListener('touchmove', function(e) {
              if (e.touches.length === 1 && startY !== undefined) {
                var deltaY = Math.abs(e.touches[0].clientY - startY);
                var deltaX = Math.abs(e.touches[0].clientX - startX);
                
                // If vertical movement is greater than horizontal, allow scrolling
                if (deltaY > deltaX && deltaY > 10) {
                  e.stopPropagation();
                  // Don't prevent default to allow scrolling
                } else if (deltaX > deltaY) {
                  // Horizontal swipe - prevent to stop carousel
                  e.preventDefault();
                }
              }
            }, { passive: false });
          }
        }
      }, 1500);
    });
  }
</script>

